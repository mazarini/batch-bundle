{
    "name": "mazarini/batch-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle providing ETL batch processing functionality",
    "keywords": [
        "ETL",
        "batch",
        "Symfony",
        "pipeline",
        "transform",
        "load",
        "extract"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Mazarini",
            "email": "mazarini@pm.me"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfony/monolog-bundle": "^3.0"
    },
    "require-dev": {
        "doctrine/dbal": "^4.3",
        "friendsofphp/php-cs-fixer": "^3.87",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^11.0",
        "symfony/debug-bundle": "7.3.*",
        "symfony/flex": "^2.8.2",
        "symfony/maker-bundle": "^1.64",
        "symfony/stopwatch": "7.3.*"
    },
    "autoload": {
        "psr-4": {
            "Mazarini\\BatchBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/app/",
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": false,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "npm install"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "fix": "vendor/bin/php-cs-fixer fix",
        "lint": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.dist.neon",
        "test": "vendor/bin/phpunit",
        "dist-upgrade": [
            "composer update",
            "npm update",
            "npm install"
        ],
        "check": [
            "composer validate --strict",
            "@lint",
            "@phpstan",
            "@test"
        ],
        "prettier": "npx prettier --write ."
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.3"
        }
    },
    "version": "dev-master"
}
